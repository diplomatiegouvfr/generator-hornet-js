{
  "themeUrl": "<%= themeurl %>",
  "fullSpa": {
    "enabled": <%= mode %>,
    "host": "<%= servicesurl %>",
    "name": "<%= _.slugify(appname) %>-service",
    "staticPath": ""
  },
  "dispatcher": {
    "stores": {
      "PageInformationStore": {
        "currentUser": {
          "name": "admin",
          "roles": [
            {
              "id": 1,
              "name": "<%= _.slugify(appname) %>_ADMIN"
            },
            {
              "id": 2,
              "name": "<%= _.slugify(appname) %>_USER"
            }
          ]
        }
      },
      "NavigationBaseStore": {
        "menuItems": []
      }
    }
  },
  "locale": "fr-FR",
  "mode": "development",
  "AppSharedProps": {
    "appName": "<%= appname %>",
    "appVersion": "<%= appversion %>",
    "appDescription": "<%= appdescription %>",
    "appAuthor": "MAEDI",
    "loginUrl": "/login",
    "logoutUrl": "/logout",
    "welcomePageUrl": "/accueil"
  }
}
