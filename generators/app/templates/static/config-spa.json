{
  "contextPath": "<%= slugify(appname) %>-spa",
  "fullSpa": {
    "enabled": true,
    "host": "http://localhost/",
    "name": "<%= slugify(appname) %>-service"
  },
  "services": {
    "host": "http://localhost/",
    "name": "<%= slugify(appname) %>-service"
  },
  "themeName": "<%= theme %>",
  "localeI18n": {
    "locale": "fr-FR",
    "lang": "fr"
  },
  "logClient": {
    "remote": false,
    "level": "DEBUG",
    "appenders": [
      {
        "type": "BrowserConsole",
        "layout": {
          "type": "THIN"
        }
      },
      {
        "type": "Ajax",
        "layout": {
          "type": "BASIC"
        },
        "threshold": 100,
        "timeout": 3000,
        "url": "/log"
      }
    ]
  }
}
