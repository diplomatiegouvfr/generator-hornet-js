{
  "name": "<%= slugify(appname) %>",
  "version": "<%= appversion %>",
  "main": "index.js",
  "description": "<%= appdescription %>",
  "tsDefinitionDependencies": {
    "hornet-js-ts-typings": "<%= fmkversion %>",
    "hornet-js-utils-dts": "<%= fmkversion %>",
    "hornet-js-core-dts": "<%= fmkversion %>",
    "hornet-js-components-dts": "<%= fmkversion %>",
    "hornet-js-react-components-dts": "<%= fmkversion %>",
    "hornet-js-passport-dts": "<%= fmkversion %>",
    "hornet-js-test-dts": "<%= fmkversion %>"
  },
  "appDependencies": {
    "<%= theme %>": "<%= themeversion %>",
    "hornet-js-components": "<%= fmkversion %>",
    "hornet-js-react-components": "<%= fmkversion %>",
    "hornet-js-core": "<%= fmkversion %>",
    "hornet-js-utils": "<%= fmkversion %>",
    "hornet-js-passport": "<%= fmkversion %>",
    "connect-flash": "0.1.1",
    "passport": "0.2.1",
